# Discord
DISCORD_BOT_TOKEN=your_discord_bot_token
DISCORD_GUILD_ID=your_server_id

# OpenClaw
OPENCLAW_API=http://localhost:18789
OPENCLAW_GATEWAY_PASSWORD=your_gateway_password

# Voice Settings  
WAKE_WORD=echo (comma-separated: echo,kimori)
ALWAYS_RESPOND=false
RESPONSE_MODE=ai

# STT Engine: 'local' (FasterWhisper) or 'elevenlabs'
STT_ENGINE=local
WHISPER_MODEL=medium (tiny, base, small, medium, large)
WHISPER_AUTOSTART=true

# TTS Engine: 'local' (gTTS) or 'elevenlabs'
TTS_ENGINE=elevenlabs
ELEVENLABS_API_KEY=your_api_key
ELEVENLABS_VOICE_ID=rachel
ELEVENLABS_MODEL=eleven_monolingual_v1
ELEVENLABS_STABILITY=0.5
ELEVENLABS_SIMILARITY=0.75

# Bot Identity
BOT_NAME=Kimori

# Thinking Phrases
THINKING_ENABLED=true
THINKING_PHRASES=hmm,let me think,uh,um,okay,give me a moment
